<h1><%= title %></h1>
<p>Welcome to <%= title %>!</p>

<a href="/new">Post new messages here!</a>

<ul>
  <% messages.forEach(message => { %>
  <li>
    <p><%= message.user %> said:</p>
    <p><%= message.text %></p>
    <p><i>Posted: <%= message.added.toUTCString() %></i></p>
  </li>
  <% }); %>
</ul>
