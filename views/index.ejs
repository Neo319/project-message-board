<h1><%= title %></h1>
<p>Welcome to <%= title %>!</p>

<a href="/new">Post new messages here!</a>

<% if (messages && messages.length) { %>
<ul>
  <% messages.forEach((message, index) => { %>
  <li>
    <p><%= message.name %> said:</p>
    <p><%= message.text %></p>
    <p><i>Posted: <%= message.added.toUTCString() %></i></p>
    <a href="/messageDetail/<%=index%>">Open</a>
  </li>
  <% }); %>
</ul>
<% }; %>
